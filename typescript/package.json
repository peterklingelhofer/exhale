{
  "name": "exhale",
  "version": "2.0.1",
  "description": "A customizable visual indicator to remind you to breathe and facilitate breathwork practice",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint ./src && prettier --check .",
    "start": "pnpm run build && electron ./dist/main.js",
    "dist": "pnpm exec electron-packager . --platform=linux,win32,darwin --overwrite",
    "dist:linux": "pnpm exec electron-packager . --platform=linux --overwrite",
    "dist:mac": "pnpm exec electron-packager . --platform=darwin --overwrite",
    "dist:windows": "pnpm exec electron-packager . --platform=win32 --overwrite",
    "format": "prettier --write ."
  },
  "build": {
    "appId": "exhale",
    "mac": {
      "category": "healthcare-fitness"
    }
  },
  "main": "./dist/main.js",
  "repository": "https://github.com/peterklingelhofer/exhale",
  "keywords": [
    "Electron",
    "typescript"
  ],
  "author": "peterklingelhofer",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron/asar": "^4.0.1",
    "@electron/packager": "^19.0.1",
    "@eslint/js": "^9.38.0",
    "commander": "^14.0.2",
    "electron": "^38.4.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "pnpm@10.19.0"
}
